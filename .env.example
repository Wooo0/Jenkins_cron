# 应用配置
PORT=3000
NODE_ENV=production

# JWT配置
JWT_SECRET=your-secret-key-change-in-production

# 管理员配置
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123
ADMIN_EMAIL=admin@example.com

# 数据库配置
DB_TYPE=sqlite
DB_PATH=./cron_jenkins.db

# MySQL配置 (当DB_TYPE=mysql时使用)
DB_HOST=localhost
DB_PORT=3306
DB_NAME=cron_jenkins
DB_USERNAME=root
DB_PASSWORD=

# PostgreSQL配置 (当DB_TYPE=postgresql时使用)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=cron_jenkins
# DB_USERNAME=postgres
# DB_PASSWORD=

# OceanBase配置 (当DB_TYPE=oceanbase时使用)
# DB_HOST=localhost
# DB_PORT=2881
# DB_NAME=cron_jenkins
# DB_USERNAME=root
# DB_PASSWORD=

# Jenkins配置
JENKINS_TIMEOUT=30000
JENKINS_RETRY_COUNT=3

# CORS配置
CORS_ORIGIN=*